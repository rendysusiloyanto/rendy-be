# Database
# SQLite (default)
# DATABASE_URL=sqlite:///./app.db

# PostgreSQL
# DATABASE_URL=postgresql://user:password@host:5432/dbname
# Contoh:
# DATABASE_URL=postgresql://ukk_user:YOUR_PASSWORD@127.0.0.1:5432/ukk_app

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/google/callback

# JWT
SECRET_KEY=your-secret-key-change-in-production

# Frontend URL
FRONTEND_URL=http://localhost:3000

# OpenVPN management (untuk traffic I/O realtime)
# OPENVPN_MANAGEMENT_HOST=127.0.0.1
# OPENVPN_MANAGEMENT_PORT=7505
# OPENVPN_POLL_INTERVAL=3

# Video upload dir (optional; default: backend/uploads/videos)
# VIDEO_UPLOAD_DIR=/path/to/videos
# HLS/DASH output dir for FFmpeg-converted streams (optional; default: backend/uploads/streams)
# VIDEO_STREAMS_DIR=/path/to/streams
# Video stream token expiry in minutes (default: 60)
# VIDEO_STREAM_TOKEN_EXPIRE_MINUTES=60

# Vertex AI (Gemini) for jns23lab AI Analyze & Assistant
# VERTEX_PROJECT_ID=your-gcp-project-id
# VERTEX_LOCATION=us-central1
# VERTEX_CREDENTIALS_PATH=./vertex.json
# GEMINI_MODEL=gemini-2.0-flash-exp

# Redis (optional; AI chat cache - Cache-Aside, DB remains source of truth)
# REDIS_URL=redis://localhost:6379/0
# CHAT_CACHE_TTL_SECONDS=86400
# CHAT_HISTORY_MAX_MESSAGES=10
