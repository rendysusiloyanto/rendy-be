# Database
# SQLite (default)
# DATABASE_URL=sqlite:///./app.db

# PostgreSQL
# DATABASE_URL=postgresql://user:password@host:5432/dbname
# Contoh:
# DATABASE_URL=postgresql://ukk_user:YOUR_PASSWORD@127.0.0.1:5432/ukk_app

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8000/api/auth/google/callback

# JWT
SECRET_KEY=your-secret-key-change-in-production

# Frontend URL
FRONTEND_URL=http://localhost:3000

# OpenVPN management (untuk traffic I/O realtime)
# OPENVPN_MANAGEMENT_HOST=127.0.0.1
# OPENVPN_MANAGEMENT_PORT=7505
# OPENVPN_POLL_INTERVAL=3

# Video upload dir (optional; default: backend/uploads/videos)
# VIDEO_UPLOAD_DIR=/path/to/videos
# Video stream token expiry in minutes (default: 60)
# VIDEO_STREAM_TOKEN_EXPIRE_MINUTES=60
